{
  "input": [
    {
      "type": "v2rayGeoIPDat",
      "action": "add",
      "args": {
        "uri": "https://github.com/Loyalsoldier/v2ray-rules-dat/releases/latest/download/geoip.dat",
        // "uri": "https://cdn.jsdelivr.net/gh/Loyalsoldier/v2ray-rules-dat@release/geoip.dat",
        "wantedList": ["cloudflare", "cloudfront", "facebook", "fastly", "google", "netflix", "telegram", "twitter", "us", "hk"]
      }
    }
  ],
  "output": [
    {
      "type": "text",
      "action": "output",
      "args": {
        "outputDir": "./output/ipv4",
        "outputExtension": ".conf",
        "addPrefixInLine": "route ",
        "addSuffixInLine": " via 192.168.88.2;",
        "excludedList": ["cn", "private", "tor"],
        // "wantedList": ["private"],
        "onlyIPType": "ipv4",
        "oneFilePerList": true
      }
    },
    {
      "type": "text",
      "action": "output",
      "args": {
        "outputDir": "./output/ipv6",
        "outputExtension": ".conf",
        "addPrefixInLine": "route ",
        "addSuffixInLine": " via fe80::7445:d7ff:fed1:ff96%br-lan;",
        "excludedList": ["cn", "private", "tor"],
        // "wantedList": ["private"],
        "onlyIPType": "ipv6",
        "oneFilePerList": true
      }
    }
  ]
}
